      <variable initial_value="0.35" name="eta" units="dimensionless"/>
      <variable initial_value="0.1" name="k_sat" units="dimensionless"/>
      <variable cmeta:id="id_00025" initial_value="38.5" name="g_NCX" units="micromolar_per_ms"/>
      <variable name="Na_i" public_interface="in" units="micromolar"/>
      <variable name="Na_o" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00027" name="Ca_i" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00026" name="Ca_o" public_interface="in" units="micromolar"/>
      <variable cmeta:id="Na_Ca_Exchanger_I_NaCa" name="I_NaCa" public_interface="out" units="micromolar_per_ms"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_NaCa</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <ci>g_NCX</ci>
                  <apply>
                     <minus/>
                     <apply>
                        <times/>
                        <apply>
                           <exp/>
                           <apply>
                              <times/>
                              <ci>eta</ci>
                              <ci>FVRT</ci>
                           </apply>
                        </apply>
                        <apply>
                           <power/>
                           <ci>Na_i</ci>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">3</cn>
                        </apply>
                        <ci>Ca_o</ci>
                     </apply>
                     <apply>
                        <times/>
                        <apply>
                           <exp/>
                           <apply>
                              <times/>
                              <apply>
                                 <minus/>
                                 <ci>eta</ci>
                                 <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">1</cn>
                              </apply>
                              <ci>FVRT</ci>
                           </apply>
                        </apply>
                        <apply>
                           <power/>
                           <ci>Na_o</ci>
                           <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">3</cn>
                        </apply>
                        <ci>Ca_i</ci>
                     </apply>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <apply>
                     <plus/>
                     <apply>
                        <power/>
                        <ci>Na_o</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">3</cn>
                     </apply>
                     <apply>
                        <power/>
                        <ci>K_mNa</ci>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">3</cn>
                     </apply>
                  </apply>
                  <apply>
                     <plus/>
                     <ci>Ca_o</ci>
                     <ci>K_mCa</ci>
                  </apply>
                  <apply>
                     <plus/>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">1</cn>
                     <apply>
                        <times/>
                        <ci>k_sat</ci>
                        <apply>
                           <exp/>
                           <apply>
                              <times/>
                              <apply>
                                 <minus/>
                                 <ci>eta</ci>
                                 <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">1</cn>
                              </apply>
                              <ci>FVRT</ci>
                           </apply>
                        </apply>
                     </apply>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00028" name="SERCA">
      <variable cmeta:id="id_00029" initial_value="0.45" name="g_SERCA" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00030" initial_value="0.5" name="K_SERCA" units="micromolar"/>
      <variable cmeta:id="SERCA_I_SERCA" name="I_SERCA" public_interface="out" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00031" name="Ca_i" public_interface="in" units="micromolar"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_SERCA</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <ci>g_SERCA</ci>
                  <apply>
                     <power/>
                     <ci>Ca_i</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">2</cn>
                  </apply>
               </apply>
               <apply>
                  <plus/>
                  <apply>
                     <power/>
                     <ci>K_SERCA</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">2</cn>
                  </apply>
                  <apply>
                     <power/>
                     <ci>Ca_i</ci>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">2</cn>
                  </apply>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00037" name="Sarcolemmal_Ca_pump">
      <variable cmeta:id="id_00038" initial_value="0.0035" name="g_pCa" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00039" initial_value="0.5" name="K_mpCa" units="micromolar"/>
      <variable name="Ca_i" public_interface="in" units="micromolar"/>
      <variable cmeta:id="Sarcolemmal_Ca_pump_I_pCa" name="I_pCa" public_interface="out" units="micromolar_per_ms"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_pCa</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <ci>g_pCa</ci>
                  <ci>Ca_i</ci>
               </apply>
               <apply>
                  <plus/>
                  <ci>K_mpCa</ci>
                  <ci>Ca_i</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00040" name="Background_Ca_current">
      <variable name="V" public_interface="in" units="mV"/>
      <variable name="Vm" public_interface="in" units="mV"/>
      <variable name="R" public_interface="in" units="mJ_per_mole_K"/>
      <variable name="T" public_interface="in" units="kelvin"/>
      <variable name="F" public_interface="in" units="C_per_mole"/>
      <variable name="Ca_i" public_interface="in" units="micromolar"/>
      <variable name="Ca_o" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00041" name="E_Ca" public_interface="out" units="mV"/>
      <variable cmeta:id="id_00042" initial_value="2.42e-5" name="g_CaB" units="micromolar_per_mV_ms"/>
      <variable cmeta:id="Background_Ca_current_I_CaB" name="I_CaB" public_interface="out" units="micromolar_per_ms"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>E_Ca</ci>
            <apply>
               <times/>
               <apply>
                  <divide/>
                  <apply>
                     <times/>
                     <ci>R</ci>
                     <ci>T</ci>
                  </apply>
                  <apply>
                     <times/>
                     <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">2</cn>
                     <ci>F</ci>
                  </apply>
               </apply>
               <apply>
                  <ln/>
                  <apply>
                     <divide/>
                     <ci>Ca_o</ci>
                     <ci>Ca_i</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>I_CaB</ci>
            <apply>
               <times/>
               <ci>g_CaB</ci>
               <apply>
                  <minus/>
                  <ci>E_Ca</ci>
                  <ci>Vm</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00043" name="SR_Ca_leak_current">
      <variable cmeta:id="id_00044" name="Ca_i" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00045" name="Ca_SR" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00046" initial_value="1.8951e-5" name="g_SRl" units="per_ms"/>
      <variable cmeta:id="SR_Ca_leak_current_I_SR" name="I_SR" public_interface="out" units="micromolar_per_ms"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_SR</ci>
            <apply>
               <times/>
               <ci>g_SRl</ci>
               <apply>
                  <minus/>
                  <ci>Ca_SR</ci>
                  <ci>Ca_i</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00047" name="troponin_Ca_buffer">
      <variable cmeta:id="id_00048" initial_value="0.04" name="k_m_TRPN" units="per_ms"/>
      <variable cmeta:id="id_00049" initial_value="0.04" name="k_p_TRPN" units="per_micromolar_ms"/>
      <variable cmeta:id="id_00050" initial_value="70" name="B_TRPN" units="micromolar"/>
      <variable cmeta:id="id_00051" name="TRPN" public_interface="in" units="micromolar"/>
      <variable name="Ca_i" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00052" name="I_TRPN" public_interface="out" units="micromolar_per_ms"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>I_TRPN</ci>
            <apply>
               <minus/>
               <apply>
                  <times/>
                  <ci>k_m_TRPN</ci>
                  <apply>
                     <minus/>
                     <ci>B_TRPN</ci>
                     <ci>TRPN</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <ci>k_p_TRPN</ci>
                  <ci>TRPN</ci>
                  <ci>Ca_i</ci>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00053" name="calmodulin_Ca_buffer">
      <variable cmeta:id="id_00054" initial_value="2.382" name="k_CMDN" units="micromolar"/>
      <variable cmeta:id="id_00055" initial_value="50" name="B_CMDN" units="micromolar"/>
      <variable name="Ca_i" public_interface="in" units="micromolar"/>
      <variable cmeta:id="id_00056" name="beta_CMDN" public_interface="out" units="dimensionless"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>beta_CMDN</ci>
            <apply>
               <power/>
               <apply>
                  <plus/>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">1</cn>
                  <apply>
                     <divide/>
                     <apply>
                        <times/>
                        <ci>k_CMDN</ci>
                        <ci>B_CMDN</ci>
                     </apply>
                     <apply>
                        <power/>
                        <apply>
                           <plus/>
                           <ci>k_CMDN</ci>
                           <ci>Ca_i</ci>
                        </apply>
                        <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">2</cn>
                     </apply>
                  </apply>
               </apply>
               <apply>
                  <minus/>
                  <cn xmlns:cellml="http://www.cellml.org/cellml/1.1#" cellml:units="dimensionless">1</cn>
               </apply>
            </apply>
         </apply>
      </math>
   </component>
   <component cmeta:id="id_00016" name="intracellular_ion_concentrations">
      <variable units="micromolar_per_ms" name="dTropTot" public_interface="in"/>
      <variable units="micromolar_per_ms" name="dTropTot2"/>
      <variable cmeta:id="id_00071" initial_value="10" name="Na_i" public_interface="out" units="micromolar"/>
      <variable cmeta:id="intracellular_ion_concentrations_Ca_i" name="Ca_i" public_interface="out" initial_value="0.1" units="micromolar"/> <!-- 0.000109675653885715 -->
      <variable cmeta:id="id_00067" initial_value=" 700" name="Ca_SR" public_interface="out" units="micromolar"/> <!-- 0.68819225819108 --> <!-- non steady state = 7003-3-->
      <variable cmeta:id="id_00057" initial_value="63.5953608442369" name="TRPN" public_interface="out" units="micromolar"/> <!-- 0.0638844336444102 -->
      <variable name="time" public_interface="in" units="ms"/>
      <variable name="V_myo" public_interface="in" units="um3"/>
      <variable cmeta:id="id_00066" name="V_SR" public_interface="in" units="um3"/>
      <variable cmeta:id="id_00065" name="beta_CMDN" public_interface="in" units="dimensionless"/>
      <variable cmeta:id="id_00064" name="I_SERCA" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00063" name="I_pCa" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00062" name="I_CaB" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00061" name="I_TRPN" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00060" name="I_SR" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00059" name="I_NaCa" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00010" name="I_RyR" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="id_00058" name="I_LCC" public_interface="in" units="micromolar_per_ms"/>
      <variable cmeta:id="intracellular_ion_concentrations_CaSR_plot" name="CaSR_plot" units="micromolar"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>dTropTot2</ci>
                <apply>
                <times/> <ci>dTropTot</ci> <!-- <apply> <minus/> --> <cn cellml:units="dimensionless">1</cn> <!-- </apply> -->
                </apply> 
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>TRPN</ci>
            </apply>
            <ci>I_TRPN</ci>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Ca_i</ci>
            </apply>
            <apply>
               <times/>
               <ci>beta_CMDN</ci>
               <apply>
                  <plus/>
                  <apply>
                     <minus/>
                     <apply>
                        <plus/>
                        <apply>
                           <minus/>
                           <apply>
                              <plus/>
                              <ci>I_LCC</ci>
                              <ci>I_RyR</ci>
                           </apply>
                           <ci>I_SERCA</ci>
                        </apply>
                        <ci>I_SR</ci>
                        <ci>I_NaCa</ci>
                     </apply>
                     <ci>I_pCa</ci>
                  </apply>
                  <ci>I_CaB</ci>
                  
                   <apply> <minus/>  <ci>dTropTot</ci>  </apply> 
                  
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Ca_SR</ci>
            </apply>
            <apply>
               <times/>
               <apply>
                  <divide/>
                  <ci>V_myo</ci>
                  <ci>V_SR</ci>
               </apply>
               <apply>
                  <minus/>
                  <apply>
                     <plus/>
                     <apply>
                        <minus/>
                        <ci>I_RyR</ci>
                     </apply>
                     <ci>I_SERCA</ci>
                  </apply>
                  <ci>I_SR</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>CaSR_plot</ci>
            <apply>
               <divide/>
               <apply>
                  <times/>
                  <ci>Ca_SR</ci>
                  <ci>V_SR</ci>
               </apply>
               <ci>V_myo</ci>
            </apply>
         </apply>
      </math>
   </component>
